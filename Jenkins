pipeline {
    agent any
    tools {
	    maven "Maven 3.8.4"
	    jdk "JDK17"
	}

    stages{
        stage('Checkout'){
            steps{
                git branch: 'main' url: 'https://github.com/AmeyD090/calculator.git'
            }
        }
        stage('Build'){
            sh 'mvn clean'
        }
    }
}
  
